{
  "name": "@usedapp/rainbowkit",
  "version": "0.5.3",
  "description": "The best way to connect a wallet",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/rainbowkit/src/index.d.ts",
  "sideEffects": [
    "src/css/reset.css.ts"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "scripts": {
    "build": "pnpm typegen && node build.js",
    "build:watch": "node build.js --watch",
    "dev": "pnpm build:watch & pnpm typegen:watch",
    "typecheck": "pnpm tsc --noEmit",
    "typegen": "tsc --emitDeclarationOnly",
    "typegen:watch": "tsc --emitDeclarationOnly --watch"
  },
  "keywords": [
    "ens",
    "ethereum",
    "react",
    "react-hook",
    "hooks",
    "blockchain"
  ],
  "author": "Rainbow",
  "license": "MIT",
  "peerDependencies": {
    "ethers": ">=5.5.1",
    "react": ">=17",
    "react-dom": ">=17",
    "wagmi": "0.5.x || 0.6.x",
    "@usedapp/core": "*"
  },
  "devDependencies": {
    "@walletconnect/ethereum-provider": "^1.8.0",
    "@vanilla-extract/esbuild-plugin": "2.1.0",
    "esbuild": "0.15.8",
    "postcss-prefix-selector": "1.16.0",
    "recursive-readdir-files": "2.3.0",
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/providers": "^5.5.1",
    "@types/qrcode": "^1.4.2",
    "@vanilla-extract/private": "^1.0.2",
    "@vanilla-extract/css-utils": "0.1.2",
    "@usedapp/core": "workspace:*",
    "autoprefixer": "^10.4.0",
    "ethers": "^5.0.0",
    "nock": "^13.2.4",
    "postcss": "^8.4.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "vitest": "^0.5.0",
    "typescript": "^4.6.2",
    "wagmi": "0.6.6",
    "@wagmi/core": "^0.4.2",
    "@metamask/detect-provider": "^1.2.0",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.1"
  },
  "dependencies": {
    "@vanilla-extract/css": "1.7.0",
    "@vanilla-extract/dynamic": "2.0.2",
    "@vanilla-extract/sprinkles": "1.4.1",
    "@ethersproject/units": "5.6.0",
    "clsx": "1.1.1",
    "qrcode": "1.5.0",
    "react-remove-scroll": "2.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbow-me/rainbowkit.git",
    "directory": "packages/rainbowkit"
